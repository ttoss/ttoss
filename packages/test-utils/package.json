{
  "name": "@ttoss/test-utils",
  "version": "1.16.1",
  "description": "This package provides a number of utilities and re-exports for testing using Jest, React Testing Library, and Storybook.",
  "keywords": [
    "Jest",
    "React",
    "React Testing Library",
    "Storybook"
  ],
  "author": "ttoss",
  "contributors": [
    {
      "email": "pedro@arantespp.com",
      "name": "Pedro Arantes",
      "url": "https://arantespp.com/contact"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./faker": {
      "import": "./dist/esm/faker.js",
      "require": "./dist/faker.js"
    },
    "./relay": {
      "import": "./dist/esm/relay.js",
      "require": "./dist/relay.js"
    },
    "./storybook": {
      "import": "./dist/esm/storybook.js",
      "require": "./dist/storybook.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "faker": [
        "./dist/faker.d.ts"
      ],
      "relay": [
        "./dist/relay.d.ts"
      ],
      "storybook": [
        "./dist/storybook.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/jest": "^11.9.3",
    "@faker-js/faker": "^7.3.0",
    "@storybook/addon-storyshots": "^6.5.9",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/relay-test-utils": "^6.0.5",
    "jest-environment-jsdom": "^28.1.1",
    "relay-test-utils": "^14.0.0"
  },
  "peerDependencies": {
    "@storybook/client-api": ">=6.5.9",
    "@storybook/react": ">=6.5.9",
    "jest": ">=28.1.1",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@ttoss/config": "^1.16.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
