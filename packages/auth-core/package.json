      },
      "./amazon-cognito": {
        "import": "./dist/esm/AmazonCognito/index.js",
        "require": "./dist/AmazonCognito/index.js",
        "types": "./dist/AmazonCognito/index.d.ts"
      }
    },
    "provenance": true
