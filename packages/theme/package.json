{
  "name": "@ttoss/theme",
  "version": "1.5.3",
  "description": "Theme for packages.",
  "license": "UNLICENSED",
  "author": "ttoss",
  "contributors": [
    "Rayza Oliveira <rayza.ocr@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ttoss/ttoss.git",
    "directory": "packages/theme"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./Bruttal": {
      "import": "./dist/esm/themes/Bruttal/Bruttal.js",
      "require": "./dist/themes/Bruttal/Bruttal.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "sideEffects": true,
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@iconify-icon/react": "^1.0.7",
    "@iconify/icons-carbon": "^1.2.18",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@iconify/icons-eos-icons": "^1.2.6",
    "@iconify/icons-mdi": "^1.2.46",
    "@iconify/types": "^2.0.0",
    "@ttoss/config": "workspace:^",
    "@ttoss/test-utils": "workspace:^",
    "jest": "^29.5.0",
    "theme-ui": "^0.16.0",
    "tsup": "^7.0.0"
  },
  "keywords": [],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "Bruttal": [
        "./dist/themes/Bruttal/Bruttal.d.ts"
      ]
    }
  }
}
