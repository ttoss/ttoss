{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "//#lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", ".next/**", "storybook-static/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "deploy": {
      "dependsOn": ["carlin#build", "//#lint", "build", "test", "^deploy"],
      "outputs": []
    }
  }
}
